{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "moduleResolution": "node",
    "module": "commonjs",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true
  }
}
